{
  "name": "league-funny",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "turbo run postinstall",
    "db-push": "db-push --filter @wanin/db",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "clean": "find . -name node_modules -o -name .next -type d -prune | xargs rm -rf",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:e2e": "turbo run test:e2e",
    "e2e:chrome": "turbo run e2e:chrome",
    "e2e:firefox": "turbo run e2e:firefox",
    "e2e:edge": "turbo run e2e:edge"
  },
  "dependencies": {
    "@wanin/db": "workspace:*"
  },
  "devDependencies": {
    "cypress": "^10.9.0",
    "@wanin/eslint-config-base": "workspace:*",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "ts-jest": "^29.0.3",
    "turbo": "latest",
    "vitest": "^0.23.4",
    "vue-jest": "^3.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.0"
}
