{
  "name": "league-funny",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "db-push": "db-push --filter @wanin/db",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "clean": "find . -name node_modules -o -name .next -type d -prune | xargs rm -rf",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:e2e": "turbo run test:e2e",
    "e2e:chrome": "turbo run e2e:chrome",
    "e2e:firefox": "turbo run e2e:firefox",
    "e2e:edge": "turbo run e2e:edge"
  },
  "devDependencies": {
    "@wanin/eslint-config-base": "workspace:0.0.0",
    "cypress": "^10.10.0",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "shell-quote": "^1.7.4",
    "ts-jest": "^29.0.3",
    "turbo": "1.6.3",
    "vitest": "^0.23.4",
    "typescript": "^4.8.4",
    "@typescript-eslint/parser": "^5.40.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "eslint": "^8.25.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.0"
}
